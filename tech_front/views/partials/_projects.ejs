
    <br>
    <h2> <%= entity %> </h2>
    <hr>
    <br>
    
    <div class="list-group">
    <% categories.forEach(function(category) { %>
      <br>
      <h3><%= category.name %> </h3>
      <div class="list-group">
      <% category.projects.forEach(function(project) { %>
        <br>
        <a href="<%= fronthostname + '/projects2/' + project.id %>" class="list-group-item">
        <h4 class="list-group-item-heading"> <%= project.title %> </h4>
        <p class="list-group-item-text"> <%= project.description %> </p>
        </a>

      <% }); %>
      </div>
    <% }); %>
<br>
<hr>
